{
  "env": {
    "browser": true,
    "node": false
  },
  "extends": ["plugin:react/recommended", "plugin:react/jsx-runtime"],
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "react/prop-types": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
